<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.dongq.android.rssreader"
    android:versionCode="2"
    android:versionName="0.0.2" >

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <provider android:name=".RssFeedContentProvider" android:authorities="org.dongq.android.rssreader.provider" android:exported="false"/>
        
        <activity
            android:name="org.dongq.android.rssreader.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="org.dongq.android.rssreader.RssItemListActivity"/>
        <activity android:name="org.dongq.android.rssreader.RssContentActivity"/>
    </application>

</manifest>
